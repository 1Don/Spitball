<% if @incoming.any? %>

  <% @incoming.each do |request| %>
    <div>
      <%=link_to request.user.name, user_path(request.user), class:"profile-link" %>
      <%= link_to "Accept Friend Request",  friend_request_path(friend_id: request.friend_id, user_id: request.user_id, id: request.id), method:"put" %>
    </div>
  <% end %>

<% else %>
  <p>You don't have any friend requests</p>
<% end %>

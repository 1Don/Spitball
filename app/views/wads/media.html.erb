<div class="col-12 mt-4">
	<div class="row">
		<%# The display for all wads%>
		<div class="col-xs-12 col-8 mb-5 pr-0 pl-5">
			<div class="col-11 align-items-center mb-4 row-flex">
				<%= render 'category_nav' %>
			</div>	
			<div class="col-11">
				<% @wads.each do |wad| %>
					<%= render wad %>
				<% end %>
			</div>
		</div>
		<div class="col-4 mt-5">
			<%= render 'wad_form' %>
		</div>
	</div>
</div>
